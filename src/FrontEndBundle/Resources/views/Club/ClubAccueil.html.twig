{% extends 'FrontEndBundle::Layout.html.twig' %}




{% block vies %} class="my-active"{% endblock vies %}

{% block body %}



    <section id="slider">
        <div class="static-banner relative">
            {% block slider %}
            {% endblock %}

            <div class="overlay blue-overlay-5"></div>
            <div class="page-head">
                <h2 class="page-title">
                    {% block nom %}Clubs{% endblock nom %}
                </h2>
                <ul class="page-title-btn">
                    <li><a href="#" target="_blank">Home <i class="fa fa-caret-right" aria-hidden="true"></i></a></li>
                    <li><a href="#" class="active">
                            {% block nome %}List Club{% endblock nome %}
                        </a></li>
                    {% block club %} {% endblock club %}
                    {% block modifier %}{% endblock %}

                </ul>
            </div>
        </div>
    </section>



    {% block list %}
        {#liste des club dans le template#}
    {% endblock list %}



{% endblock body %}